{
  "passPerPreset": true,
  "presets": [
    {
      "plugins": [
        "transform-runtime",
      ]
    },
    {
      "passPerPreset": false,
      "presets": [
        "react",
        [
          "es2015",
          { modules: false
          }
        ],
        "stage-0",
        "flow"
      ]
    }
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "transform-regenerator",
    "transform-runtime",
    "transform-es2015-modules-commonjs",
  ],
}