    "description": "project гост звук",
    "main": "index.js",
    "scripts": {
        "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js --ignore client/ --ignore webpack/webpack-assets.json",
        "start:prod": "cross-env  BABEL_DISABLE_CACHE=1 NODE_ENV=production node index.js",
        "bs": "yarn clean && yarn build && yarn build:server && yarn start:prod",
        "build": "yarn clean && yarn build:client && yarn build:server",
        "build:client": "yarn clean:client && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
        "build:server": "yarn clean:server && cross-env NODE_ENV=production BABEL_DISABLE_CACHE=1 webpack --config ./webpack/webpack.config.server.js",
        "clean": "rimraf build",
        "clean:client": "rimraf public/static/dist/client",
        "clean:server": "rimraf build/server",
        "slate": "rimraf node_modules && yarn install",
        "lint": "eslint client server",
        "test:watch": "jest --watch",
        "test": "jest",
        "validate": "npm ls",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.js": [
            "prettier --single-quote --write",
            "git add"
        ]
    },
    "pre-commit": [
        "test",
        "precommit"
    ],
